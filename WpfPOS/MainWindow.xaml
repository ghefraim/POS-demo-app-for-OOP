<Window x:Class="WpfPOS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPOS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FF4A595B">
        <TextBox x:Name="textBox" HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="584" Height="264"/>

        <Button x:Name="getProduseFromXmlButtonc" Content="GET PRODUSE FROM XML" HorizontalAlignment="Left" Margin="43,338,0,0" VerticalAlignment="Top" Height="48" Width="171" Background="#FFC1D09C" Click="getProduseFromXmlButton_Click"/>
        <Button x:Name="getServiciiFromXmlButton" Content="GET SERVICII FROM XML" HorizontalAlignment="Center" Margin="0,338,0,0" VerticalAlignment="Top" Height="48" Width="172" Background="#FFC1D09C" Click="getServiciiFromXmlButton_Click"/>
        <Button x:Name="getPacheteFromXmlButton" Content="GET PACHETE FROM XML" HorizontalAlignment="Right" Margin="0,338,39,0" VerticalAlignment="Top" Height="48" Width="171" Background="#FFC1D09C" Click="getPacheteFromXmlButton_Click"/>
    </Grid>
</Window>
